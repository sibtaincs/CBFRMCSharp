    }


    List<CBFRM.Areas.ComplainManagement.Models.spGetForm_Result> spGetForm = new List<CBFRM.Areas.ComplainManagement.Models.spGetForm_Result>();
    spGetForm = db.spGetForm(RoleID, 1).ToList();


}



